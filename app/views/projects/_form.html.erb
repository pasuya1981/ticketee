<%= form_for @project do |f| %>
  
  <%= render 'shared/model_error', obj: @project %>
  
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>

  <%= f.submit %>
<% end %>